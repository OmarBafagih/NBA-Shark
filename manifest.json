{
    "name": "NBA Shark",
    "version": "0.0.0.1",
    "description": "view the scores of your favourite teams anytime.",
    "permissions":[],
    "icons":{"128":"icon2.png", "48":"icon2.png", "16":"icon2.png"},
    
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["main_page.js"]
        }
    ],

    "action": {
        "default_popup":"main_page.html",
        "default_title": "NBA-extension",
        "default_icon":"icon2.png"
    },
    
    "manifest_version": 3
   
  }